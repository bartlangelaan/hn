{
  "extends": [
    "config:js-lib",
    ":semanticCommits",
    ":semanticCommitTypeAll(build)",
    ":semanticCommitScopeDisabled",
    ":rebaseStalePrs"
  ],
  "automergeType": "branch-push",
  "devDependencies": {
    "automerge": true
  },
  "packageRules": [
    {
      "packageNames": ["query-string"],
      "allowedVersions": "< 6 || >= 7"
    }
  ]
}
